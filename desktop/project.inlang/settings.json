{
	"baseLocale": "en-US",
	"locales": ["en-US", "ca-AD", "es-ES", "es-MX", "fr-FR", "he-IL", "hi-IN", "it-IT", "ja-JP", "ko-KR", "no-NO", "pl-PL", "pt-BR", "ru-RU", "sv-SE", "tr-TR", "vi-VN", "zh-CN", "zh-HK"],
	"modules": [
		"https://cdn.jsdelivr.net/npm/@inlang/plugin-i18next@latest/dist/index.js"
	],
	"plugin.inlang.i18next": {
		"pathPattern": {
			"common": "./src-tauri/locales/{locale}/common.json",
			"language": "./src-tauri/locales/{locale}/language.json"
		}
	}
}
